{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#BBC4B9",
					"leading_diamond": " ",
					"properties": {
						"display_host": true
					},
					"style": "diamond",
					"template": "{{ .UserName }} ",
					"type": "session"
				},
				{
					"foreground": "#BFA8BF",
					"properties": {
						"branch_icon": "\ue725 ",
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true,
						"fetch_worktree_count": true
					},
					"style": "diamond",
					"template": "<gray>{{ .HEAD }}</gray>{{ if .Working.Changed }} <magenta>*</magenta>{{ end }}",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"foreground": "#FFF",
					"foreground_templates": ["{{ if gt .Code 0 }}red{{ end }}"],
					"properties": {
						"always_enabled": false
					},
					"style": "plain",
					"template": "{{.Code}}",
					"type": "status"
				},
				{
					"foreground": "#fff",
					"properties": {
						"style": "austin",
						"threshold": 0
					},
					"style": "diamond",
					"template": " <#BBC4B9>{{ .FormattedMs }}s</> <#fff>\udb80\udddd</>",
					"type": "executiontime"
				},
				{
					"properties": {
						"root_icon": "\uf292 "
					},
					"style": "diamond",
					"template": " \uf0e7",
					"type": "root"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "cyan",
					"leading_diamond": "",
					"properties": {
						"folder_icon": "\u002F",
						"folder_separator_icon": "\u002F",
						"home_icon": "~",
						"style": "agnoster_full"
					},
					"style": "diamond",
					"template": "{{ .Path }}",
					"trailing_diamond": "",
					"type": "path"
				},
				{
					"foreground": "#FFF",
					"foreground_templates": ["{{ if gt .Code 0 }}red{{ end }}"],
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": " \udb81\ude27 ",
					"type": "status"
				}
			],
			"type": "prompt"
		}
	],
	"console_title_template": "{{ .PWD }}",
	"version": 3
}
